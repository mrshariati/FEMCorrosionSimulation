#Finite Element
cell = triangle;
H = FiniteElement("CG", cell, 1)

ci = TrialFunction(H)
v = TestFunction(H)

Di = Constant(cell)
zi = Constant(cell)
phi = Coefficient(H)
Ii = Coefficient(H)
Ri = Coefficient(H)

F = 96487
R = 8.3145
T = 295

um = F/(R*T)

#Create stiffness matrix
a = Di*inner(grad(ci),grad(v))*dx - zi*Di*um*ci*inner(grad(phi),grad(v))*dx
L = Ri*v*dx - Ii*v*ds

